{
    conn <- getConn()
    on.exit(doneWith(conn))
    names <- names(list(...))
    values <- list(...)
    query <- paste("SELECT * FROM Sessions WHERE", paste0(names, 
        " = :", names))
    result <- dbGetQuery(conn, query, params = values)
    result$datetime <- as.POSIXct(result$datetime, origin = "1970-01-01", 
        tz = "CST6CDT")
    result
  }
}
